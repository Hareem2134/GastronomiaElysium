{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "functions": {
      "dist/index.js": {
        "runtime": "node"
      }
    },
    "rewrites": [
      { "source": "/api/(.*)", "destination": "/dist/index.js" },
      { "source": "/(.*)", "destination": "/dist/public/index.html" }
    ]
  }